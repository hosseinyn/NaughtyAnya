{
  "expo": {
    "name": "NaughtyAnya",
    "slug": "NaughtyAnya",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/images/anya.png",
    "scheme": "naughtyanya",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.hosseinyn.NaughtyAnya"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/anya.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5a99cd67-5a31-4359-85cd-5d897a4eb398"
      }
    }
  }
}
